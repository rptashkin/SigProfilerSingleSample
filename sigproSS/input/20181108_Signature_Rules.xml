<?xml version="1.0" encoding="utf-8" ?>
<signatureRules>

	<signatureName signatureSBS = "SBS5">
		<strandBias>
			<mutationType mutType = "T>C">
				<direction>-1</direction>
				<pValue>0.01</pValue>
			</mutationType>
	    </strandBias>
	</signatureName>

	<signatureName signatureSBS = "SBS31">
		<strandBias>
			<mutationType mutType = "C>T">
				<direction>-1</direction>
				<pValue>0.01</pValue>
			</mutationType>
	    </strandBias>
	</signatureName>

<alwaysIncludeSignatures>
	<signature>SBS1</signature>
	<signature>SBS5</signature>
</alwaysIncludeSignatures>

</signatureRules>
